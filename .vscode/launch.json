{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "üì¶ Build Executable (PyInstaller)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/utils/build_exe.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "name": "üöÄ Run Debug",
      "type": "debugpy",
      "request": "launch",
      "module": "imfont_compressor.main",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "üöÄ Run Executable Build",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/dist/imfont_compressor.exe",
      "args": [],
      "stopAtEntry": false
    },
    {
      "name": "‚¨ÜÔ∏è Upload Executable to GitHub",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/utils/upload_release.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "GITHUB_TOKEN": "${env:GITHUB_TOKEN}"
      }
    }
  ]
}
